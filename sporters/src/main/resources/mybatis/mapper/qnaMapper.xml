<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gdu.sporters.userSupport.mapper.QnaMapper">

	<select id="selectAllQnaCount" resultType="int">
		SELECT COUNT(*)
		  FROM QNA
	</select>
	
	<select id="selectAllQnaList" parameterType="Map" resultType="QnaDTO">
		SELECT USER_NO, QNA_TITLE, QNA_PW, QNA_CONTENT, QNA_CREATE_DATE, STATE, DEPTH, GROUP_NO, FAQ_STATE
		  FROM QNA
		 WHERE #{begin} AND #{end}
		  ORDER BY QNA_NO DESC
	</select>

	
</mapper>